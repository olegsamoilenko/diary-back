{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "watchIgnore": [
    "**/node_modules/**",
    "**/dist/**",
    "**/.git/**",
    "**/.idea/**",
    "**/.vscode/**",
    "**/logs/**",
    "**/tmp/**",
    "**/*.log"
  ]
}